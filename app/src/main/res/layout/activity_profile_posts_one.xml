<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnellipsesix"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <FrameLayout android:id="@+id/frameStacktelevision"
            android:layout_height="@dimen/_286pxv" android:layout_width="match_parent">
            <LinearLayout android:id="@+id/linearColumntelevision"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_width="match_parent"
                android:orientation="vertical" style="@style/groupStylegreen_400">
                <androidx.appcompat.widget.Toolbar
                    android:id="@+id/toolbarToolbar"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_width="match_parent" app:contentInsetStart="0dp">
                    <LinearLayout android:id="@+id/linearIOSStatusBar"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <ImageView android:id="@+id/imageTelevision"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginTop="@dimen/_17pxv"
                            android:layout_width="@dimen/_28pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_television"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_television"/>
                        <FrameLayout android:id="@+id/frameStacknotch"
                            android:layout_height="@dimen/_28pxv"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginStart="@dimen/_16pxh" android:layout_width="@dimen/_282pxh">
                            <ImageView android:id="@+id/imageNotch"
                                android:layout_gravity="start"
                                android:layout_height="@dimen/_28pxh"
                                android:layout_marginBottom="@dimen/_1pxv"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="@dimen/_219pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_notch"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_notch"/>
                            <ImageView android:id="@+id/imageSettings"
                                android:layout_gravity="bottom|end"
                                android:layout_height="@dimen/_11pxh"
                                android:layout_marginStart="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_10pxv"
                                android:layout_width="@dimen/_66pxh"
                                android:scaleType="fitXY"
                                android:src="@drawable/img_settings_white_a700"
                                tools:ignore="ContentDescription" tools:src="@drawable/img_settings_white_a700"/>
                        </FrameLayout>
                    </LinearLayout>
                </androidx.appcompat.widget.Toolbar>
                <LinearLayout android:id="@+id/linearPageHeader"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_148pxv"
                    android:layout_marginEnd="@dimen/_14pxh"
                    android:layout_marginStart="@dimen/_14pxh"
                    android:layout_marginTop="@dimen/_30pxv"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSettingsOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.3"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_settings"
                        style="@style/txtIntermedium16_4" tools:text="@string/lbl_settings"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtProfile"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_62pxh"
                        android:layout_weight="0.44"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_profile"
                        style="@style/txtIntersemibold30_1" tools:text="@string/lbl_profile"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtLogout"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_7pxv"
                        android:layout_marginStart="@dimen/_69pxh"
                        android:layout_marginTop="@dimen/_9pxv"
                        android:layout_weight="0.25"
                        android:layout_width="0dp"
                        android:singleLine="true"
                        android:text="@string/lbl_logout"
                        style="@style/txtIntermedium16_4" tools:text="@string/lbl_logout"/>
                </LinearLayout>
            </LinearLayout>
            <ImageView android:id="@+id/imageEllipseSix"
                android:layout_gravity="bottom|center"
                android:layout_height="@dimen/_158pxh"
                android:layout_marginEnd="@dimen/_108pxh"
                android:layout_marginStart="@dimen/_108pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_158pxh"
                android:scaleType="centerCrop" circular="@{true}"
                imageRes="@{@drawable/img_ellipse6}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse6"/>
        </FrameLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearProfilePostsO"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtVictoriaRobert"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_victoria_robert"
                style="@style/txtIntersemibold30" tools:text="@string/msg_victoria_robert"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtAmantragoesh"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_8pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/msg_a_mantra_goes_h"
                style="@style/txtIntersemibold16_1" tools:text="@string/msg_a_mantra_goes_h"/>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearSegmentedContr"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_22pxv"
                android:layout_width="match_parent"
                android:orientation="horizontal" style="@style/groupStylegray_1">
                <FrameLayout android:id="@+id/frameStacksearch"
                    android:layout_height="@dimen/_46pxv"
                    android:layout_marginBottom="@dimen/_2pxv"
                    android:layout_marginStart="@dimen/_2pxh"
                    android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_171pxh">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSearch"
                        android:layout_gravity="center"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_12pxv"
                        android:layout_marginEnd="@dimen/_40pxh"
                        android:layout_marginStart="@dimen/_40pxh"
                        android:layout_marginTop="@dimen/_13pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@string/lbl_search"
                        style="@style/txtIntermedium16_1" tools:text="@string/lbl_search"/>
                    <androidx.appcompat.widget.AppCompatButton
                        android:ellipsize="end" android:gravity="center"
                        android:id="@+id/btnPosts"
                        android:layout_gravity="start"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent"
                        android:paddingBottom="@dimen/_13pxv"
                        android:paddingEnd="@dimen/_30pxh"
                        android:paddingStart="@dimen/_30pxh"
                        android:paddingTop="@dimen/_13pxv"
                        android:singleLine="true"
                        android:text="@string/lbl_posts"
                        android:textAllCaps="false"
                        style="@style/btnSolidRounded_1" tools:text="@string/lbl_posts"/>
                </FrameLayout>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPhotos"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_61pxh"
                    android:layout_marginStart="@dimen/_54pxh"
                    android:layout_marginTop="@dimen/_15pxv"
                    android:layout_weight="0.24"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_photos"
                    style="@style/txtIntermedium16_1" tools:text="@string/lbl_photos"/>
            </LinearLayout>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListcontentblock"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_27pxh}" tools:itemCount="3" tools:listitem="@layout/row_listcontentblock"/>
            <View android:background="@color/gray_201"
                android:id="@+id/lineDividerLine"
                android:layout_gravity="end"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_277pxh"/>
            <FrameLayout android:gravity="start"
                android:id="@+id/frameStackcontentblockThree"
                android:layout_height="@dimen/_83pxv"
                android:layout_marginTop="@dimen/_10pxv" android:layout_width="match_parent">
                <LinearLayout android:id="@+id/linearContentContent"
                    android:layout_gravity="bottom|center"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_16pxh"
                    android:layout_marginStart="@dimen/_16pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <LinearLayout
                        android:id="@+id/linearRowcontentblockThree"
                        android:layout_height="wrap_content"
                        android:layout_width="match_parent" android:orientation="horizontal">
                        <View
                            android:background="@drawable/rectangle_bg_gray_100_radius_8"
                            android:id="@+id/viewContentBlockThree"
                            android:layout_height="@dimen/_50pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginTop="@dimen/_1pxv" android:layout_width="@dimen/_50pxh"/>
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnheaderThree"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="@dimen/_16pxh"
                            android:layout_weight="0.85"
                            android:layout_width="0dp" android:orientation="vertical">
                            <LinearLayout
                                android:id="@+id/linearRowheaderThree"
                                android:layout_height="wrap_content"
                                android:layout_width="match_parent" android:orientation="horizontal">
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtHeaderThree"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_weight="0.53"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@string/lbl_header"
                                    style="@style/txtIntersemibold16_1" tools:text="@string/lbl_header"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDistanceThree"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_170pxh"
                                    android:layout_marginTop="@dimen/_4pxv"
                                    android:layout_weight="0.47"
                                    android:layout_width="0dp"
                                    android:singleLine="true"
                                    android:text="@string/lbl_8m_ago"
                                    style="@style/txtInterregular14_1" tools:text="@string/lbl_8m_ago"/>
                            </LinearLayout>
                            <TextView
                                android:id="@+id/txtHellwanttouThree"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_6pxv"
                                android:layout_width="match_parent"
                                android:text="@string/msg_he_ll_want_to_u"
                                style="@style/txtInterregular14_2" tools:text="@string/msg_he_ll_want_to_u"/>
                        </LinearLayout>
                    </LinearLayout>
                    <View android:background="@color/gray_201"
                        android:id="@+id/lineDividerLineOne"
                        android:layout_height="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_12pxv" android:layout_width="@dimen/_277pxh"/>
                </LinearLayout>
                <LinearLayout android:id="@+id/linearIOSBottomBar"
                    android:layout_gravity="start"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="vertical">
                    <View android:background="@color/bluegray_200"
                        android:id="@+id/lineTopDivider"
                        android:layout_height="@dimen/_1pxv" android:layout_width="@dimen/_375pxh"/>
                    <FrameLayout android:id="@+id/frameStackbackground"
                        android:layout_height="@dimen/_83pxv" android:layout_width="match_parent">
                        <View android:background="@color/gray_50"
                            android:id="@+id/viewBackground"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_83pxv" android:layout_width="@dimen/_375pxh"/>
                        <ImageView android:id="@+id/imageTabs"
                            android:layout_gravity="top|center"
                            android:layout_height="@dimen/_32pxh"
                            android:layout_marginBottom="@dimen/_14pxv"
                            android:layout_marginEnd="@dimen/_26pxh"
                            android:layout_marginStart="@dimen/_26pxh"
                            android:layout_marginTop="@dimen/_14pxv"
                            android:layout_width="@dimen/_320pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_tabs"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_tabs"/>
                    </FrameLayout>
                </LinearLayout>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
