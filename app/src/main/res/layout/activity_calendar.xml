<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumntwentynine"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <LinearLayout android:gravity="end"
            android:id="@+id/linearColumntelevision"
            android:layout_height="wrap_content"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylegreen_400">
            <androidx.appcompat.widget.Toolbar
                android:id="@+id/toolbarToolbar"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_width="match_parent" app:contentInsetStart="0dp">
                <LinearLayout android:id="@+id/linearIOSStatusBar"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent" android:orientation="horizontal">
                    <ImageView android:id="@+id/imageTelevision"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="@dimen/_28pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_television"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_television"/>
                    <FrameLayout android:id="@+id/frameStacknotch"
                        android:layout_height="@dimen/_28pxv"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginStart="@dimen/_16pxh" android:layout_width="@dimen/_282pxh">
                        <ImageView android:id="@+id/imageNotch"
                            android:layout_gravity="start"
                            android:layout_height="@dimen/_28pxh"
                            android:layout_marginBottom="@dimen/_1pxv"
                            android:layout_marginEnd="@dimen/_10pxh"
                            android:layout_width="@dimen/_219pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_notch"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_notch"/>
                        <ImageView android:id="@+id/imageSettings"
                            android:layout_gravity="bottom|end"
                            android:layout_height="@dimen/_11pxh"
                            android:layout_marginStart="@dimen/_10pxh"
                            android:layout_marginTop="@dimen/_10pxv"
                            android:layout_width="@dimen/_66pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_settings_white_a700"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_settings_white_a700"/>
                    </FrameLayout>
                </LinearLayout>
            </androidx.appcompat.widget.Toolbar>
            <TextView android:ellipsize="end" android:id="@+id/txtMarch"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@string/lbl_march"
                style="@style/txtIntersemibold30_1" tools:text="@string/lbl_march"/>
            <LinearLayout android:id="@+id/linearDaysofWeek"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_14pxh"
                android:layout_marginStart="@dimen/_14pxh"
                android:layout_marginTop="@dimen/_31pxv"
                android:layout_width="match_parent" android:orientation="horizontal">
                <TextView android:ellipsize="end"
                    android:id="@+id/txtMo"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.17"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_mo"
                    style="@style/txtInterregular14_3" tools:text="@string/lbl_mo"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTu"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.13"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_tu"
                    style="@style/txtInterregular14_3" tools:text="@string/lbl_tu"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtWe"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.17"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_we"
                    style="@style/txtInterregular14_3" tools:text="@string/lbl_we"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtTh"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_weight="0.14"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_th"
                    style="@style/txtInterregular14_3" tools:text="@string/lbl_th"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtFr"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_weight="0.11"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_fr"
                    style="@style/txtInterregular14_3" tools:text="@string/lbl_fr"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSa"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.13"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_sa"
                    style="@style/txtInterregular14_3" tools:text="@string/lbl_sa"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtSu"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_36pxh"
                    android:layout_marginTop="@dimen/_1pxv"
                    android:layout_weight="0.14"
                    android:layout_width="0dp" android:singleLine="true"
                    android:text="@string/lbl_su"
                    style="@style/txtInterregular14_3" tools:text="@string/lbl_su"/>
            </LinearLayout>
        </LinearLayout>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearCalendar"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_32pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700">
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListtwentynine"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_39pxh}" tools:itemCount="5" tools:listitem="@layout/row_listtwentynine"/>
            <androidx.recyclerview.widget.RecyclerView
                android:gravity="center_horizontal"
                android:id="@+id/recyclerListicon"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_30pxv"
                android:layout_width="match_parent"
                android:orientation="vertical"
                app:layoutManager="androidx.recyclerview.widget.LinearLayoutManager"
                itemSpace="@{@dimen/_31pxh}" tools:itemCount="4" tools:listitem="@layout/row_listicon1"/>
            <View android:background="@color/gray_201"
                android:id="@+id/lineDivider"
                android:layout_gravity="center_horizontal"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_15pxv" android:layout_width="@dimen/_343pxh"/>
            <View android:background="@color/bluegray_200"
                android:id="@+id/lineTopDivider"
                android:layout_height="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_78pxv" android:layout_width="@dimen/_375pxh"/>
            <FrameLayout android:id="@+id/frameStackbackground"
                android:layout_height="@dimen/_83pxv" android:layout_width="match_parent">
                <View android:background="@color/gray_50"
                    android:id="@+id/viewBackground"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_83pxv" android:layout_width="@dimen/_375pxh"/>
                <ImageView android:id="@+id/imageTabs"
                    android:layout_gravity="top|center"
                    android:layout_height="@dimen/_32pxh"
                    android:layout_marginBottom="@dimen/_14pxv"
                    android:layout_marginEnd="@dimen/_26pxh"
                    android:layout_marginStart="@dimen/_26pxh"
                    android:layout_marginTop="@dimen/_14pxv"
                    android:layout_width="@dimen/_320pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_tabs"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_tabs"/>
            </FrameLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
