<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="end"
        android:id="@+id/linearMarketOne"
        android:layout_height="match_parent"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStylewhite_A700">
        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbarToolbar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_width="match_parent" app:contentInsetStart="0dp">
            <LinearLayout android:id="@+id/linearIOSStatusBar"
                android:layout_height="wrap_content"
                android:layout_width="match_parent" android:orientation="horizontal">
                <ImageView android:id="@+id/imageNineHundredFortyOne"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginTop="@dimen/_17pxv"
                    android:layout_width="@dimen/_28pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_941"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_941"/>
                <FrameLayout android:id="@+id/frameStacknotch"
                    android:layout_height="@dimen/_28pxv"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginStart="@dimen/_16pxh" android:layout_width="@dimen/_282pxh">
                    <ImageView android:id="@+id/imageNotch"
                        android:layout_gravity="start"
                        android:layout_height="@dimen/_28pxh"
                        android:layout_marginBottom="@dimen/_1pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_width="@dimen/_219pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_notch"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_notch"/>
                    <ImageView android:id="@+id/imageSettings"
                        android:layout_gravity="bottom|end"
                        android:layout_height="@dimen/_11pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_10pxv"
                        android:layout_width="@dimen/_66pxh"
                        android:scaleType="fitXY"
                        android:src="@drawable/img_settings"
                        tools:ignore="ContentDescription" tools:src="@drawable/img_settings"/>
                </FrameLayout>
            </LinearLayout>
        </androidx.appcompat.widget.Toolbar>
        <LinearLayout android:id="@+id/linearPageHeader"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_34pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <TextView android:ellipsize="end" android:id="@+id/txtBack"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.17" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_back"
                style="@style/txtIntermedium16" tools:text="@string/lbl_back"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMessages"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_61pxh"
                android:layout_weight="0.66" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_messages"
                style="@style/txtIntersemibold30" tools:text="@string/lbl_messages"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtFilter"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_12pxv"
                android:layout_marginStart="@dimen/_56pxh"
                android:layout_marginTop="@dimen/_4pxv"
                android:layout_weight="0.17" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_filter"
                style="@style/txtIntermedium16" tools:text="@string/lbl_filter"/>
        </LinearLayout>
        <FrameLayout android:id="@+id/frameMessageReceive"
            android:layout_gravity="start"
            android:layout_height="@dimen/_126pxv"
            android:layout_marginEnd="@dimen/_124pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_28pxv" android:layout_width="@dimen/_235pxh">
            <ImageView android:id="@+id/imageBG"
                android:layout_gravity="start"
                android:layout_height="@dimen/_126pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY" android:src="@drawable/img_bg"
                tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
            <TextView android:gravity="top|start"
                android:id="@+id/txtLanguage"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_lorem_ipsum_dol"
                style="@style/txtInterregular14_2" tools:text="@string/msg_lorem_ipsum_dol"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameMessageSend"
            android:layout_height="@dimen/_86pxv"
            android:layout_marginEnd="@dimen/_19pxh"
            android:layout_marginStart="@dimen/_19pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_235pxh">
            <ImageView android:id="@+id/imageVector"
                android:layout_gravity="start"
                android:layout_height="@dimen/_86pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            <TextView android:gravity="top|start"
                android:id="@+id/txtLanguageOne"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_lorem_ipsum_dol2"
                style="@style/txtInterregular14_3" tools:text="@string/msg_lorem_ipsum_dol2"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameMessageSend1"
            android:layout_height="@dimen/_86pxv"
            android:layout_marginEnd="@dimen/_18pxh"
            android:layout_marginStart="@dimen/_18pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_235pxh">
            <ImageView android:id="@+id/imageVectorOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_86pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY"
                android:src="@drawable/img_vector"
                tools:ignore="ContentDescription" tools:src="@drawable/img_vector"/>
            <TextView android:gravity="top|start"
                android:id="@+id/txtLanguageTwo"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_lorem_ipsum_dol2"
                style="@style/txtInterregular14_3" tools:text="@string/msg_lorem_ipsum_dol2"/>
        </FrameLayout>
        <FrameLayout android:id="@+id/frameMessageReceive1"
            android:layout_gravity="start"
            android:layout_height="@dimen/_126pxv"
            android:layout_marginEnd="@dimen/_124pxh"
            android:layout_marginStart="@dimen/_16pxh"
            android:layout_marginTop="@dimen/_16pxv" android:layout_width="@dimen/_235pxh">
            <ImageView android:id="@+id/imageBGOne"
                android:layout_gravity="start"
                android:layout_height="@dimen/_126pxh"
                android:layout_width="match_parent"
                android:scaleType="fitXY" android:src="@drawable/img_bg"
                tools:ignore="ContentDescription" tools:src="@drawable/img_bg"/>
            <TextView android:gravity="top|start"
                android:id="@+id/txtLanguageThree"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_15pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_15pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_lorem_ipsum_dol"
                style="@style/txtInterregular14_2" tools:text="@string/msg_lorem_ipsum_dol"/>
        </FrameLayout>
        <EditText
            android:background="@drawable/rectangle_bg_gray_100_border_gray_201_radius_25"
            android:drawableEnd="@drawable/drawable_img_vector_white_a700"
            android:drawablePadding="@dimen/_30pxh"
            android:ellipsize="end"
            android:hint="@string/lbl_message_here"
            android:id="@+id/etInputSearch" android:inputType="text"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_16pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_146pxv"
            android:layout_width="match_parent"
            android:paddingBottom="@dimen/_13pxv"
            android:paddingEnd="@dimen/_8pxh"
            android:paddingStart="@dimen/_16pxh"
            android:paddingTop="@dimen/_16pxv" android:singleLine="true"
            android:textColorHint="@color/gray_400"
            style="@style/etSolidRoundedOutline_1" tools:ignore="Autofill"/>
    </LinearLayout>
</layout>
