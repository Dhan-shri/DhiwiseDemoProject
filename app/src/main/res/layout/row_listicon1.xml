<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:id="@+id/linearRowtime"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="horizontal">
        <LinearLayout android:id="@+id/linearRowicon"
            android:layout_height="wrap_content"
            android:layout_weight="0.59" android:layout_width="0dp" android:orientation="horizontal">
            <View
                android:background="@drawable/rectangle_bg_green_400_radius_8"
                android:id="@+id/viewIcon"
                android:layout_height="@dimen/_16pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_2pxv" android:layout_width="@dimen/_16pxh"/>
            <TextView android:ellipsize="end" android:id="@+id/txtItem"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_weight="0.68" android:layout_width="0dp"
                android:singleLine="true"
                android:text="@string/lbl_item"
                style="@style/txtIntermedium16_3" tools:text="@string/lbl_item"/>
        </LinearLayout>
        <TextView android:ellipsize="end" android:id="@+id/txtTime"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_1pxv"
            android:layout_marginStart="@dimen/_231pxh"
            android:layout_marginTop="@dimen/_1pxv"
            android:layout_weight="0.41" android:layout_width="0dp"
            android:singleLine="true" android:text="@string/lbl_30min"
            style="@style/txtInterregular14_2" tools:text="@string/lbl_30min"/>
    </LinearLayout>
</layout>
