<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnhotdeals"
        android:layout_height="wrap_content"
        android:layout_width="match_parent" android:orientation="vertical">
        <TextView android:ellipsize="end" android:id="@+id/txtHotdeals"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_width="wrap_content"
            android:singleLine="true"
            android:text="@string/lbl_hot_deals"
            style="@style/txtIntermedium24" tools:text="@string/lbl_hot_deals"/>
        <LinearLayout android:gravity="start"
            android:id="@+id/linearRowrectanglethirty"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_10pxh"
            android:layout_marginTop="@dimen/_15pxv"
            android:layout_width="match_parent" android:orientation="horizontal">
            <LinearLayout android:gravity="start"
                android:id="@+id/linearContentMarket3"
                android:layout_height="wrap_content"
                android:layout_weight="0.35" android:layout_width="0dp" android:orientation="vertical">
                <View
                    android:background="@drawable/rectangle_bg_gray_100_radius_8"
                    android:id="@+id/viewRectangleThirty"
                    android:layout_height="@dimen/_110pxh" android:layout_width="@dimen/_110pxh"/>
                <TextView android:id="@+id/txtItem1NameGo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@string/msg_item_1_name_go"
                    style="@style/txtInterregular14_2" tools:text="@string/msg_item_1_name_go"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPrice"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_19_99"
                    style="@style/txtIntersemibold14" tools:text="@string/lbl_19_99"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearContentMarket4"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_weight="0.35" android:layout_width="0dp" android:orientation="vertical">
                <View
                    android:background="@drawable/rectangle_bg_gray_100_radius_8"
                    android:id="@+id/viewRectangleThirtyOne"
                    android:layout_height="@dimen/_110pxh" android:layout_width="@dimen/_110pxh"/>
                <TextView android:id="@+id/txtItem1NameGoOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@string/msg_item_1_name_go"
                    style="@style/txtInterregular14_2" tools:text="@string/msg_item_1_name_go"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceOne"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_19_99"
                    style="@style/txtIntersemibold14" tools:text="@string/lbl_19_99"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearContentMarket"
                android:layout_height="wrap_content"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_weight="0.35" android:layout_width="0dp" android:orientation="vertical">
                <ImageView android:id="@+id/imageRectangleThirtyTwo"
                    android:layout_height="@dimen/_110pxh"
                    android:layout_width="@dimen/_110pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_rectangle30}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle30"/>
                <TextView android:id="@+id/txtItem1NameGoTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@string/msg_item_1_name_go"
                    style="@style/txtInterregular14_2" tools:text="@string/msg_item_1_name_go"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceTwo"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_7pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_19_99"
                    style="@style/txtIntersemibold14" tools:text="@string/lbl_19_99"/>
            </LinearLayout>
            <LinearLayout android:gravity="start"
                android:id="@+id/linearContentMarket1"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_weight="0.35" android:layout_width="0dp" android:orientation="vertical">
                <ImageView android:id="@+id/imageRectangleThirtyThree"
                    android:layout_height="@dimen/_110pxh"
                    android:layout_width="@dimen/_110pxh"
                    android:scaleType="fitXY"
                    cornerRadius="@{@dimen/_8pxh}"
                    imageRes="@{@drawable/img_rectangle30_110x110}"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle30_110x110"/>
                <TextView android:id="@+id/txtItem1NameGoThree"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="match_parent"
                    android:text="@string/msg_item_1_name_go"
                    style="@style/txtInterregular14_2" tools:text="@string/msg_item_1_name_go"/>
                <TextView android:ellipsize="end"
                    android:id="@+id/txtPriceThree"
                    android:layout_height="wrap_content"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_8pxv"
                    android:layout_width="wrap_content"
                    android:singleLine="true"
                    android:text="@string/lbl_19_99"
                    style="@style/txtIntersemibold14" tools:text="@string/lbl_19_99"/>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</layout>
