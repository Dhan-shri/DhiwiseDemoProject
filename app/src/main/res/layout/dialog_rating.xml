<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data/>
    <LinearLayout android:gravity="end" android:id="@+id/linearRating"
        android:layout_height="match_parent"
        android:layout_width="match_parent" android:orientation="vertical">
        <LinearLayout android:gravity="end"
            android:id="@+id/linearIOSStatusBar"
            android:layout_height="wrap_content"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_width="match_parent" android:orientation="horizontal">
            <ImageView android:id="@+id/imageTelevision"
                android:layout_height="@dimen/_11pxh"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginTop="@dimen/_17pxv"
                android:layout_width="@dimen/_28pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_television"
                tools:ignore="ContentDescription" tools:src="@drawable/img_television"/>
            <FrameLayout android:id="@+id/frameStacknotch"
                android:layout_height="@dimen/_28pxv"
                android:layout_marginBottom="@dimen/_1pxv"
                android:layout_marginStart="@dimen/_16pxh" android:layout_width="@dimen/_282pxh">
                <ImageView android:id="@+id/imageNotch"
                    android:layout_gravity="start"
                    android:layout_height="@dimen/_28pxh"
                    android:layout_marginBottom="@dimen/_1pxv"
                    android:layout_marginEnd="@dimen/_10pxh"
                    android:layout_width="@dimen/_219pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_notch"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_notch"/>
                <ImageView android:id="@+id/imageSettings"
                    android:layout_gravity="bottom|end"
                    android:layout_height="@dimen/_11pxh"
                    android:layout_marginStart="@dimen/_10pxh"
                    android:layout_marginTop="@dimen/_10pxv"
                    android:layout_width="@dimen/_66pxh"
                    android:scaleType="fitXY"
                    android:src="@drawable/img_settings_white_a700"
                    tools:ignore="ContentDescription" tools:src="@drawable/img_settings_white_a700"/>
            </FrameLayout>
        </LinearLayout>
        <LinearLayout android:gravity="center_horizontal"
            android:id="@+id/linearPopup"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/_20pxv"
            android:layout_marginEnd="@dimen/_14pxh"
            android:layout_marginStart="@dimen/_14pxh"
            android:layout_marginTop="@dimen/_164pxv"
            android:layout_width="match_parent"
            android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
            <androidx.appcompat.widget.AppCompatRatingBar
                android:id="@+id/ratingBarRatingbar"
                android:layout_gravity="center_horizontal"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_32pxv"
                android:layout_width="wrap_content" android:numStars="5"
                android:progressTint="@color/orange_300"
                android:rating="5" style="@style/Widget.AppCompat.RatingBar.Indicator"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtRateourapp"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_34pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/lbl_rate_our_app"
                style="@style/txtIntersemibold30" tools:text="@string/lbl_rate_our_app"/>
            <TextView android:gravity="center_horizontal"
                android:id="@+id/txtDescription"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_11pxv"
                android:layout_width="match_parent"
                android:text="@string/msg_consequat_velit"
                style="@style/txtIntermedium16_2" tools:text="@string/msg_consequat_velit"/>
            <androidx.appcompat.widget.AppCompatButton
                android:ellipsize="end" android:gravity="center"
                android:id="@+id/btnILoveIt"
                android:layout_height="wrap_content"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_41pxv"
                android:layout_width="match_parent"
                android:paddingBottom="@dimen/_15pxv"
                android:paddingEnd="@dimen/_30pxh"
                android:paddingStart="@dimen/_30pxh"
                android:paddingTop="@dimen/_15pxv"
                android:singleLine="true"
                android:text="@string/lbl_i_love_it"
                android:textAllCaps="false"
                style="@style/btnSolidRounded" tools:text="@string/lbl_i_love_it"/>
            <TextView android:ellipsize="end"
                android:gravity="center_horizontal"
                android:id="@+id/txtDontlikethe"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_20pxv"
                android:layout_marginEnd="@dimen/_16pxh"
                android:layout_marginStart="@dimen/_16pxh"
                android:layout_marginTop="@dimen/_18pxv"
                android:layout_width="match_parent"
                android:singleLine="true"
                android:text="@string/msg_don_t_like_the"
                style="@style/txtIntersemibold16" tools:text="@string/msg_don_t_like_the"/>
        </LinearLayout>
    </LinearLayout>
</layout>
